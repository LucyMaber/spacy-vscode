{
  "name": "spacy-extension",
  "description": "spaCy support for .cfg files",
  "author": "Explosion AI",
  "repository": "https://github.com/explosion/spacy-vscode",
  "license": "MIT",
  "version": "0.1.0",
  "publisher": "Explosion AI",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "ms-python.python"
  ],
  "activationEvents": [
    "workspaceContains:*.cfg"
  ],
  "contributes": {
    "commands": [
      {
        "command": "spacy_test",
        "title": "spaCy Test"
      },
      {
        "command": "extension.activate",
        "title": "Activate spaCy server",
        "category": "Server management"
      },
      {
        "command": "spacy.showCurrentEnv",
        "title": "Return python environment",
        "category": "Debugging"
      }
    ],
    "configuration": {
      "title": "spaCy Server Configuration",
      "properties": {
        "spacy-extension.pythonInterpreter": {
          "scope": "resource",
          "type": "string",
          "default": "",
          "description": "Please select a python interpreter to run the server-side python code. Make sure that you have spaCy and pygls installed."
        }
      }
    }
  },
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "16.11.43",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  }
}