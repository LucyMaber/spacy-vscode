{
  "name": "spaCy_Extension",
  "description": "spaCy support for .cfg files",
  "author": "Explosion AI",
  "repository": "https://github.com/explosion",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "Explosion AI",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:*.cfg"
  ],
  "contributes": {
    "commands": [
      {
        "command": "spacy_test",
        "title": "spaCy Test"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Json Server Configuration",
      "properties": {
        "jsonServer.exampleConfiguration": {
          "scope": "resource",
          "type": "string",
          "default": "You can override this message."
        }
      }
    }
  },
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}